{
  "name": "trpc-api-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "trpc-api-export": "tsup --config trpc-api-export/builder/tsup.config.ts && npm run format-fix",
    "prepare": "husky install",
    "build": "shx rm -rf build && tsc -p tsconfig.json",
    "dev": "PORT=4000 ts-node-dev src/index.ts",
    "lint": "eslint --max-warnings 0 .",
    "lint-fix": "eslint --fix .",
    "lint-staged-husky": "lint-staged",
    "tsc": "tsc -p tsconfig.json",
    "format-lint": "prettier --config .prettierrc --check --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown -l ."
  },
  "dependencies": {
    "@trpc/server": "10.29.1",
    "cors": "2.8.5",
    "express": "4.18.2",
    "superjson": "1.12.3",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsup": "6.7.0",
    "typescript": "5.1.3"
  }
}
